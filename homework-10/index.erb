<% title = "Services list page" + page %>
<% list = ["Label","Last visited at", "Counter", "Action"] %>
<%= partial "template.erb", locals: {title: title, index:true, list:list } %>


  <% subjects.each do |subject| %>
      <tr>
        <td><%= link_to(subject.label, "/services/#{subject.id}") %></td>
        <td><%= subject.last_visited_at %></td>
        <td><%= link_to(subject.counter, "/services/#{subject.id}/visits") %></td>
        <td>
          <% form_for subject, "/services/#{subject.id}", method: :delete do |f| %><%= f.submit("Delete", "/services/#{subject.id}", class: "btn btn-danger") %>
              <%= link_to("Edit", "/services/#{subject.id}/edit", class: "btn btn-success") %>
          <% end %>

        </td>
      </tr>
  <% end %>
  </tbody>
</table>
<%= paginate subjects %>
