<% title = "Visits list for service" + label %>
<% list = ["Data","Visited at"] %>
<%= partial "template.erb", locals: {title: title, list:list} %>

<% visits.each do |visit| %>
      <tr>
        <td><%= visit.data %></td>
        <td><%= visit.created_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<%= link_to "Services", "/services" , class: "btn btn-success"%>

<%= paginate visits %>
